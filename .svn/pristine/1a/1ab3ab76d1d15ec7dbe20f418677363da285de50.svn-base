@font-face {
  font-family: 'Open Sans';
  src: url('fonts/open-sans/OpenSans.eot');
  src: url('fonts/open-sans/OpenSans.eot?#iefix') format('embedded-opentype'), 
  url('fonts/open-sans/OpenSans.woff2') format('woff2'), 
  url('fonts/open-sans/OpenSans.woff') format('woff'), 
  url('fonts/open-sans/OpenSans.ttf') format('truetype'), 
  url('fonts/open-sans/OpenSans.svg#OpenSans') format('svg');
}
body{
	font-family: 'Open Sans', arial;
	background:url(images/bg.png);
	color:#555;
	font-size: 14px;
  	line-height: 1.42857143;
  	margin: 0;
}
button, input[type="reset"], input[type="button"], input[type="submit"] {
  	background: #00ACE7;
  	border:none;
  	padding: 10px 15px;
	backface-visibility: hidden;
	box-shadow: 0 0 1px rgba(0, 0, 0, 0);
	display: inline-block;
	position: relative;
	transform: translateZ(0px);
	transition-duration: 0.3s;
	transition-property: color;
	vertical-align: middle;
	border-radius:5px;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	color:#fff;
	text-decoration:none;
}
input[type="reset"]:hover::before, input[type="button"]:hover::before, input[type="submit"]:hover::before, button:hover::before {
	transform: scaleX(1);
	background:#533A71;
	border-radius:5px;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	text-decoration:none;
}
input[type="reset"]::before, input[type="button"]::before, input[type="submit"]::before, button::before{
    bottom: 0;
    content: "";
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    transform: scaleX(0);
    transform-origin: 0 50% 0;
    transition-duration: 0.3s;
    transition-property: transform;
    transition-timing-function: ease-out;
    z-index: -1;
}
input, button, select, textarea {
	padding: 8px 10px;
}
